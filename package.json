{
  "name": "@searchkings/gulp-gcs-upload",
  "version": "1.0.8",
  "description": "Gulp plugin to upload to Google Cloud Storage",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/SearchKings/gulp-gcs-upload",
  "license": "MIT",
  "keywords": [
    "gulp",
    "google",
    "cloud",
    "storage",
    "upload",
    "typescript"
  ],
  "scripts": {
    "semantic-release": "semantic-release",
    "lint": "npx eslint --config eslint.config.ts \"src/**/*.ts\"",
    "build": "npx tsc",
    "start": "npx tsc -w",
    "test": "npx jest --config jest.config.ts",
    "prettier:all": "npx prettier --write \"**/*ts\"",
    "prettier:check:all": "npx prettier --check --config ./.prettierrc \"**/*ts\"",
    "prettier:staged": "npx pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit:verify"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.17.0",
    "ansi-colors": "^4.1.3",
    "fancy-log": "^2.0.0",
    "lodash": "^4.17.21",
    "mime-types": "^3.0.1",
    "plugin-error": "^2.0.1",
    "through2-concurrent": "^2.0.0",
    "vinyl": "^3.0.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/event-stream": "^4.0.5",
    "@types/fancy-log": "^2.0.2",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.3.0",
    "@types/through2-concurrent": "^2.0.4",
    "@types/vinyl": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "event-stream": "^4.0.1",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "semantic-release": "^24.2.7",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
