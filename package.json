{
  "name": "@searchkings/gulp-gcs-upload",
  "version": "0.0.0",
  "description": "Gulp plugin to upload to Google Cloud Storage",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/SearchKings/gulp-gcs-upload",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "lint": "npx eslint \"src/**/*.ts\"",
    "build": "npx tsc",
    "start": "npx tsc -w",
    "prettier:all": "npx prettier --write \"**/*ts\"",
    "prettier:check:all": "npx prettier --check --config ./.prettierrc \"**/*ts\"",
    "prettier:staged": "npx pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit:verify"
    }
  },
  "engines": {
    "node": "12"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.8.4",
    "ansi-colors": "^4.1.1",
    "fancy-log": "^1.3.3",
    "mime-types": "^2.1.30",
    "plugin-error": "^1.0.1",
    "through2": "^4.0.2"
  },
  "devDependencies": {
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.14.41",
    "@types/through2": "^2.0.36",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "typescript": "^4.2.4"
  },
  "private": true
}
