{
  "name": "@searchkings/gulp-gcs-upload",
  "version": "1.0.6-beta-2024.0",
  "description": "Gulp plugin to upload to Google Cloud Storage",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/SearchKings/gulp-gcs-upload",
  "license": "MIT",
  "keywords": [
    "gulp",
    "google",
    "cloud",
    "storage",
    "upload",
    "typescript"
  ],
  "scripts": {
    "semantic-release": "semantic-release",
    "lint": "npx eslint \"src/**/*.ts\"",
    "build": "npx tsc",
    "start": "npx tsc -w",
    "test": "npx jest",
    "prettier:all": "npx prettier --write \"**/*ts\"",
    "prettier:check:all": "npx prettier --check --config ./.prettierrc \"**/*ts\"",
    "prettier:staged": "npx pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit:verify"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.11.2",
    "ansi-colors": "^4.1.3",
    "fancy-log": "^2.0.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "plugin-error": "^2.0.1",
    "through2-concurrent": "^2.0.0",
    "vinyl": "^3.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/event-stream": "^4.0.5",
    "@types/fancy-log": "^2.0.2",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.6",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.14.9",
    "@types/through2-concurrent": "^2.0.4",
    "@types/vinyl": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "event-stream": "^4.0.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "pretty-quick": "^4.0.0",
    "semantic-release": "^24.0.0",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.3"
  }
}
